{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"strict": true,
		"jsx": "preserve",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"sourceMap": true,
		"skipLibCheck": true,
		"importHelpers": true,
		"allowSyntheticDefaultImports": true,
		"useDefineForClassFields": true,
		"resolveJsonModule": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["/*"]
		 },
		"lib": [
			"esnext",
			"dom"
		],
		"types": [
			"@dcloudio/types"
		]
	},
	"exclude": [
		"node_modules",
		"unpackage",
		"src/**/*.nvue"
	],
	"include":["*.d.ts"]
}